# CortexDB Configuration
# Copy this file to .env and update the values

# Admin API Key - Get this from the gateway startup logs
# Format: cortexdb_admin_[random-string]
CORTEXDB_ADMIN_KEY=cortexdb_admin_91f733debd...

# Gemini API Key for embeddings (optional)
GEMINI_API_KEY=your-gemini-api-key-here

# Database Configuration
POSTGRES_DB=cortex
POSTGRES_USER=cortex
POSTGRES_PASSWORD=cortex_pass

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin123

# Qdrant Configuration
QDRANT_API_KEY=your-qdrant-api-key-here
